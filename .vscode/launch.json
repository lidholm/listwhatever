  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development (chrome)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--target",
        "lib/main_development.dart",
        "-d",
        "chrome"
      ]
    },
    {
      "name": "Launch development (android)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart",
        "-d",
        "emulator-5554"
      ]
    },
    {
      "name": "Launch development (macos)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart",
        "-d",
        "macos"
      ]
    },
    {
      "name": "Launch development (ios)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": [
        "--flavor",
        "development",
        "--target",
        "lib/main_development.dart",
        "-d",
        "0DB6149C-54C5-4A75-BBD7-D5195742CEC9"
      ]
    },
    {
      "name": "Launch staging",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "args": ["--flavor", "staging", "--target", "lib/main_staging.dart"]
    },
    {
      "name": "Launch production",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": ["--flavor", "production", "--target", "lib/main_production.dart"]
    },
    {
      "name": "Integration Test",
      "request": "launch",
      "type": "dart",
      "program": "integration_test/add_list_test.dart",
      "args": ["--flavor", "development", "-d", "emulator-5554"]
    }
  ]
}
